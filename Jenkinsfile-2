pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:latest' }
      }
      steps {
        bat 'mvn --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'ubuntu' }
      }
      steps {
        sh 'ubuntu --version'
      }
    }
  }
}
